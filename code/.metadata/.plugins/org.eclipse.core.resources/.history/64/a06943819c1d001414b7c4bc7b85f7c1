package ch.desm.middleware.modules.component.interlocking.obermattlangnau.maps;

import java.util.Map;

import ch.desm.middleware.modules.component.ComponentMapBase;

public class OMLUbw32Analog extends ComponentMapBase {

	//B0 = AN0, B1 = AN1
	//11bin = 3dez 
	public static final String PINBITMASK_INPUT_ANALOG = "3";
	
	public OMLUbw32Analog(){
		super();
	}
		
	public boolean isKeyAvailable(String id){
		return map.containsKey(id);
	}
	
	public boolean isValueAvailable(String id){
		return map.containsValue(id);
	}
	
	@Override
	public Map<String, String> getMap(){
		return this.map;
	}
	
	public void initialize(){

		map.put("8.91.01", "AN0#"); //FSS Grundstellung
		map.put("8.91.03", "AN0"); //FSS 10° F 
		map.put("8.91.04", "AN0"); //FSS 30° F 
		map.put("8.91.05", "AN0"); //FSS 45° F 
		map.put("8.91.06", "AN0"); //FSS 80° F 
		map.put("8.91.07", "AN0"); //FSS 90° F 
		map.put("8.91.19", "AN0"); //FSS 10° EG 
		map.put("8.91.20", "AN0"); //FSS 30° EG 
		map.put("8.91.21", "AN0"); //FSS 45° EG 
		map.put("8.91.22", "AN0"); //FSS 80° EG 
		map.put("8.91.23", "AN0"); //FSS 90° EG 

		
	}
}
